{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"mainEntryPointFilePath": "<projectFolder>/tmp/dts/main.d.ts",
	"compiler": {
		"tsconfigFilePath": "<projectFolder>/tsconfig.build-types.json"
	},
	"apiReport": {
		"enabled": false
	},
	"docModel": {
		"enabled": false
	},
	"dtsRollup": {
		"enabled": true,
		"untrimmedFilePath": "<projectFolder>/dist/main.d.ts"
	},
	"tsdocMetadata": {
		"enabled": false
	},
	"messages": {
		"compilerMessageReporting": {
			"default": {
				"logLevel": "error"
			}
		},
		"extractorMessageReporting": {
			"default": {
				"logLevel": "error"
			},
			"ae-missing-release-tag": {
				"logLevel": "none"
			}
		},
		"tsdocMessageReporting": {
			"default": {
				"logLevel": "error"
			},
			"tsdoc-param-tag-missing-hyphen": {
				"logLevel": "none"
			}
		}
	}
}
