{
	"name": "@vanyauhalin/result",
	"version": "0.2.0",
	"description": "A Result type, inspired by Go's error model",
	"keywords": [
		"error-handling",
		"go-style",
		"result",
		"try-catch",
		"type-safe"
	],
	"homepage": "https://github.com/vanyauhalin/result/",
	"bugs": {
		"url": "https://github.com/vanyauhalin/result/issues/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vanyauhalin/result.git"
	},
	"license": "MIT",
	"author": {
		"name": "Ivan Uhalin",
		"email": "hi@vanyauhalin.me",
		"url": "https://vanyauhalin.me/"
	},
	"type": "module",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist/"
	],
	"scripts": {
		"build-compat": "tsc --project tsconfig.build-compat.json",
		"build-main": "esbuild --bundle --format=esm --log-level=error --outfile=dist/main.js --platform=neutral --target=esnext lib/main.ts",
		"build-package-json": "clear-package-json package.json --output package.json",
		"build-types": "tsc --project tsconfig.build-types.json && api-extractor run",
		"lint-code": "eslint --no-warn-ignored lib *.*",
		"lint-types": "tsc",
		"test": "node lib/main.test.ts"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "7.52.11",
		"@types/node": "24.3.0",
		"@vanyauhalin/eslint-config": "0.0.12",
		"clean-publish": "5.2.2",
		"esbuild": "0.25.9",
		"eslint": "9.39.2",
		"expect-type": "1.3.0",
		"typescript": "5.9.2"
	},
	"packageManager": "pnpm@10.15.0",
	"engines": {
		"node": ">=16.0.0"
	}
}
